{
  "name": "vertebrae",
  "version": "1.1.2",
  "description": "Backbone as CommonJS modules",
  "main": "index.js",
  "scripts": {
    "test": "phantomjs test/vendor/runner.js test/index.html?noglobals=true",
    "build": "browserify index.js -s Vertebrae | derequire > backbone.js",
