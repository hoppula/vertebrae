  },
  "author": "Lari Hoppula <lari@hoppula.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.5.0",
    "native-promise-only": "^0.7.6-a"
  },
  "devDependencies": {
    "browserify": "^8.1.1",
    "derequire": "^1.2.0",
    "phantomjs": "^1.9.15",
    "uglify-js": "^2.4.16",
    "uglifyify": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hoppula/vertebrae.git"
  }
}
